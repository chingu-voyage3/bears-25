{
  "name": "codestories",
  "description": "A platform for developers to share their story, experience, motivation, tutorials, etc along the coding journey.",
  "author": "bears-25 (Chingu)",
  "private": true,
  "scripts": {
    "dev": "node server",
    "start": "cross-env NODE_ENV=production node server",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "lint": "eslint src",
    "test": "mocha --compilers babel-register src/**/*.test.js src/**/*.spec.js"
  },
  "engines": {
    "node": ">=8.9.1",
    "npm": ">=4.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "compression": "^1.7.1",
    "cross-env": "^5.1.1",
    "dotenv": "^4.0.0",
    "es6-promise": "^4.1.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "firebase": "^4.8.1",
    "lru-cache": "^4.1.1",
    "serve-favicon": "^2.4.5",
    "vue": "^2.5.3",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.3",
    "vue2-medium-editor": "^1.1.2",
    "vuetify": "^0.17.5",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chokidar": "^1.7.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "memory-fs": "^0.4.1",
    "mocha": "^4.0.1",
    "rimraf": "^2.6.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.5.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.3",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0"
  }
}
